{"ast":null,"code":"export default {\n  name: 'TaskList',\n  props: ['tasks'],\n  methods: {\n    updateTask(task) {\n      this.$emit('update-task', task);\n    },\n    deleteTask(taskId) {\n      this.$emit('delete-task', taskId);\n    },\n    formatDate(dateString) {\n      // Bug 6: Incorrect date format (displays MM-DD-YYYY instead of YYYY-MM-DD)\n      const date = new Date(dateString);\n      return `${date.getMonth() + 1}-${date.getDate()}-${date.getFullYear()}`;\n    }\n  }\n};","map":{"version":3,"names":["name","props","methods","updateTask","task","$emit","deleteTask","taskId","formatDate","dateString","date","Date","getMonth","getDate","getFullYear"],"sources":["C:\\Users\\user\\Downloads\\qa-ex\\frontend\\src\\components\\TaskList.vue"],"sourcesContent":["<template>\n    <ul class=\"task-list\">\n      <li v-for=\"task in tasks\" :key=\"task.id\" class=\"task-item\">\n        <div class=\"task-header\">\n          <input \n            type=\"checkbox\" \n            :checked=\"task.completed\" \n            @change=\"updateTask({ ...task, completed: !task.completed })\"\n          >\n          <span class=\"task-title\" :class=\"{ completed: task.completed }\">{{ task.title }}</span>\n          <span class=\"task-priority\" :class=\"task.priority\">{{ task.priority }}</span>\n        </div>\n        <p class=\"task-description\">{{ task.description }}</p>\n        <div class=\"task-footer\">\n          <span>Due: {{ formatDate(task.dueDate) }}</span>\n          <button @click=\"deleteTask(task.id)\" class=\"delete-btn\">Delete</button>\n        </div>\n      </li>\n    </ul>\n  </template>\n  \n  <script>\n  export default {\n    name: 'TaskList',\n    props: ['tasks'],\n    methods: {\n      updateTask(task) {\n        this.$emit('update-task', task);\n      },\n      deleteTask(taskId) {\n        this.$emit('delete-task', taskId);\n      },\n      formatDate(dateString) {\n        // Bug 6: Incorrect date format (displays MM-DD-YYYY instead of YYYY-MM-DD)\n        const date = new Date(dateString);\n        return `${date.getMonth() + 1}-${date.getDate()}-${date.getFullYear()}`;\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .task-list {\n    list-style-type: none;\n    padding: 0;\n  }\n  \n  .task-item {\n    background-color: #f9f9f9;\n    border: 1px solid #ddd;\n    border-radius: 4px;\n    margin-bottom: 10px;\n    padding: 10px;\n  }\n  \n  .task-header {\n    display: flex;\n    align-items: center;\n    margin-bottom: 5px;\n  }\n  \n  .task-title {\n    margin-left: 10px;\n    flex-grow: 1;\n  }\n  \n  .task-title.completed {\n    text-decoration: line-through;\n    color: #888;\n  }\n  \n  .task-priority {\n    padding: 2px 5px;\n    border-radius: 3px;\n    font-size: 0.8em;\n  }\n  \n  .task-priority.high {\n    background-color: #dc3545;\n    color: #fff;\n  }\n  \n  .task-priority.medium {\n    background-color: #ffc107;\n    color: #000;\n  }\n  \n  .task-priority.low {\n    background-color: #28a745;\n    color: #fff;\n  }\n  \n  .task-description {\n    margin: 5px 0;\n    color: #666;\n  }\n  \n  .task-footer {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin-top: 5px;\n    font-size: 0.9em;\n  }\n  \n  .delete-btn {\n    background-color: #dc3545;\n    color: #fff;\n    border: none;\n    padding: 5px 10px;\n    border-radius: 3px;\n    cursor: pointer;\n  }\n  \n  .delete-btn:hover {\n    background-color: #c82333;\n  }\n  </style>"],"mappings":"AAsBE,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,OAAO,CAAC;EAChBC,OAAO,EAAE;IACPC,UAAUA,CAACC,IAAI,EAAE;MACf,IAAI,CAACC,KAAK,CAAC,aAAa,EAAED,IAAI,CAAC;IACjC,CAAC;IACDE,UAAUA,CAACC,MAAM,EAAE;MACjB,IAAI,CAACF,KAAK,CAAC,aAAa,EAAEE,MAAM,CAAC;IACnC,CAAC;IACDC,UAAUA,CAACC,UAAU,EAAE;MACrB;MACA,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,OAAO,GAAGC,IAAI,CAACE,QAAQ,CAAC,IAAI,CAAC,IAAIF,IAAI,CAACG,OAAO,CAAC,CAAC,IAAIH,IAAI,CAACI,WAAW,CAAC,CAAC,EAAE;IACzE;EACF;AACF,CAAC","ignoreList":[]},"me